{
    "manifest_version": 3,
    "name": "Peer Review",
    "description": "Make Life Easier",
    "version": "1.0",
    "icons": {
        "16": "assets/img/icon-16.png",
        "32": "assets/img/icon-32.png",
        "48": "assets/img/icon-48.png",
        "128": "assets/img/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://github.com/*/*/pull/*"],
            "css": ["assets/lib/jquery-toast.css"],
            "js": ["assets/lib/jquery.js", "assets/lib/jquery-toast.js", "src/github.js"]
        }
    ]
}
